<% if !member_signed_in? %>
  <%= link_to "Sign in", new_member_session_path %>
  <%= link_to "Sign up", new_member_registration_path %>
<% else %>
  <% if !current_member.profile.new_record? %>
    <%= link_to "View Profile", member_profile_path(current_member, current_member.profile) %>
  <% end %>
  <%= link_to "Logout", destroy_member_session_path, method: :delete %>
<% end %>
