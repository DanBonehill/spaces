<% if !member_signed_in? %>
  <%= link_to "Sign in", new_member_session_path %>
  <%= link_to "Sign up", new_member_registration_path %>
<% else %>
  <% if !current_member.profile.nil? %>
    <%= link_to "View Profile", member_profile_path(member_id: current_member.id, id: current_member.profile.id) %>
  <% end %>
  <%= link_to "Logout", destroy_member_session_path, method: :delete %>
<% end %>
